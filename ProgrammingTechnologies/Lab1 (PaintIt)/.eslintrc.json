{
    "extends": "airbnb-base",
    "plugins": [
        "react"
    ],
    "parser": "babel-eslint",
    "rules": {
      "class-methods-use-this": "off",
      "no-underscore-dangle": "off",
      "global-require": "off",
      "prefer-destructuring": "off",
      "no-mixed-operators": "off",
      "no-param-reassign": "off",
      "no-unused-vars": [2, {"args": "after-used", "argsIgnorePattern": "event|flash|callConstructor"}],
      "no-empty": "off",
      "no-fallthrough": "off",
      "default-case": "off"
    },
    "globals": {
      "document": true
    }
}
